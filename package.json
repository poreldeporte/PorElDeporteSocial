{
  "name": "unistack",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "supabase"
  ],
  "scripts": {
    "_with-env": "dotenv -e .env -c -- ",
    "ios": "cd apps/expo && yarn ios",
    "ios:run": "cd apps/expo && yarn ios:run",
    "android": "cd apps/expo && yarn android",
    "native": "cd apps/expo && yarn start",
    "expo:prebuild": "cd apps/expo && yarn expo:prebuild",
    "expo": "cd apps/expo && npx expo",
    "watch": "npm-run-all --parallel check:type:watch watch:packages",
    "watch:packages": "ultra -r --no-pretty --concurrency 400 watch",
    "web": "yarn workspace next-app dev",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:analyze": "ANALYZE=true yarn web:prod",
    "web:prod:serve": "yarn workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "patch-package && yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app --exclude storybook-app run build",
    "build:ci": "yarn workspace next-app run build",
    "storybook": "yarn storybook:web",
    "sb": "yarn storybook:web",
    "storybook:web": "yarn build && cd apps/storybook && yarn dev",
    "storybook:native": "yarn build && cd apps/storybook-rn && yarn storybook",
    "storybook:ios": "yarn build && cd apps/storybook-rn && yarn ios",
    "storybook:android": "yarn build && cd apps/storybook-rn && yarn android",
    "chromatic": "yarn build && cd apps/storybook && yarn chromatic",
    "upgrade:tamagui": "yarn up '*tamagui*' '@tamagui/*'",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check-deps": "check-dependency-version-consistency .",
    "check-circular-deps": "yarn turbo check-circular-deps",
    "test": "yarn workspaces foreach --all run test",
    "supa": "yarn workspace @my/supabase",
    "typecheck": "turbo check:type",
    "check:type": "tsc -b --preserveWatchOutput tsconfig.json",
    "check:type:watch": "yarn check:type -w",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "lint-sherif": "npx --yes sherif@latest",
    "setup": "yarn create tamagui --template takeout-starter --info",
    "generate": "turbo gen",
    "gen": "yarn generate",
    "add:font": "yarn workspace tamagui-fonts-and-icons run add:font",
    "add:icon": "yarn workspace tamagui-fonts-and-icons run add:icon",
    "get-local-ip-mac": "ifconfig | grep 'inet ' | grep -v '127.0.0.1' | awk '{ print $2 }'"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "next": "15.4.3",
    "@babel/core": "^7.26.0",
    "babel-loader": "^8.3.0"
  },
  "packageManager": "yarn@4.1.0",
  "devDependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "@nderscore/tamagui-typescript-plugin": "^0.6.0",
    "@react-navigation/drawer": "^6.6.15",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tamagui/cli": "^1.132.7",
    "@tamagui/config": "^1.132.7",
    "@turbo/gen": "^2.5.3",
    "@types/fs-extra": "^11",
    "axios": "^1.7.2",
    "check-dependency-version-consistency": "^3.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.46.0",
    "fs-extra": "^11.2.0",
    "node-gyp": "^9.3.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "promise-throttle-all": "^1.1.1",
    "ts-node": "10.9.2",
    "turbo": "^2.5.3",
    "typescript": "~5.8.3",
    "ultra-runner": "^3.10.5"
  }
}
